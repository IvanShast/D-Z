using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp17
{
    class Program
    {
        static void Main(string[] args)
        {
            int end = 99;
            for (int i = 7; i < end; i += 7)
            {
                Console.WriteLine(i);
            }
        }
    }
}
