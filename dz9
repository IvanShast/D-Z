using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp17
{
    class Program
    {
        static void Main(string[] args)
        {
            int end = 99;
            int sequence = 7;
            for (int firstNumber = 7; firstNumber < end; firstNumber += sequence)
            {
                Console.WriteLine(firstNumber);
            }
        }
    }
}
